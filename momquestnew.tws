Vmacros['countdown'] = {\u000a	handler: function() {\u000a		var minutes = 15;\u000a		var seconds = 10;\u000a\u000a		var interval = setInterval(function() {\u000a			var el = document.getElementById("countdown");\u000a			if(seconds == 0) {\u000a				if(minutes == 0) {\u000a					el.innerHTML = "You panic!";\u000a					\u000a					var links = document.getElementsByClassName("internalLink");\u000a					for (var i = 0; i < links.length; i++) {\u000a						links[i].setAttribute("id", i+1);\u000a					}\u000a\u000a					window.setTimeout(followRandomLink(1, state.history[0].variables["numberOfLinks"]), 2000);\u000a					function followRandomLink(min,max) {\u000a						var linkID = Math.floor(Math.random() * (max - min + 1)) + min;\u000a						var link = document.getElementById(linkID);\u000a						state.history[0].variables["panic"] = true\u000a						link.click();\u000a					}\u000a					clearInterval(interval);\u000a					return;\u000a				} else {\u000a					minutes--;\u000a					seconds = 60;\u000a				}\u000a			}\u000a			if(minutes > 0) {\u000a				var minute_text = minutes + (minutes > 1 ? ' minutes' : ' minute');\u000a			} else {\u000a				var minute_text = '';\u000a			}\u000a			var second_text = seconds > 1 ? 'seconds' : 'second';\u000a			el.innerHTML = minute_text + ' ' + seconds + ' ' + second_text + ' remaining!';\u000a			seconds--;\u000a		}, 1000);\u000a	}\u000a}
